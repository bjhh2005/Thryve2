{
  "name": "fronted",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Workflow Editor Application",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "electron:dev": "cross-env NODE_ENV=development concurrently \"npm run dev\" \"electron .\"",
    "electron:zrx": "cross-env NODE_ENV=development concurrently \"npm run dev\" \"electron . --disable-gpu-sandbox\"",
    "electron:build": "vite build && electron-builder",
    "electron:build:zrx": "vite build && cross-env ELECTRON_EXTRA_LAUNCH_ARGS=\"--disable-gpu-sandbox\" electron-builder",
    "electron:build:portable": "vite build && cross-env ELECTRON_EXTRA_LAUNCH_ARGS=\"--disable-gpu-sandbox\" electron-builder --win portable",
    "electron:preview": "electron .",
    "build:backend": "cd ../Backend && mkdir -p workflows && pyinstaller backend.spec",
    "build:all": "npm run build && npm run build:backend && npm run electron:zrx",
    "package:all": "electron-builder --dir"
  },
  "dependencies": {
    "@flowgram.ai/form-materials": "^0.2.14",
    "@flowgram.ai/free-container-plugin": "^0.2.14",
    "@flowgram.ai/free-group-plugin": "^0.2.14",
    "@flowgram.ai/free-layout-editor": "^0.2.14",
    "@flowgram.ai/free-node-panel-plugin": "^0.2.14",
    "@flowgram.ai/free-snap-plugin": "^0.2.14",
    "@flowgram.ai/runtime-interface": "^0.2.14",
    "@flowgram.ai/runtime-js": "^0.2.14",
    "fuzzysort": "^3.1.0",
    "idb": "^8.0.3",
    "lodash-es": "^4.17.21",
    "mime-types": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-textarea-autosize": "^8.5.9",
    "remark-gfm": "^4.0.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^37.1.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "less": "^4.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "build": {
    "appId": "com.your-company.workflow-editor",
    "productName": "Thryve",
    "directories": {
      "output": "release",
      "buildResources": "build",
      "app": "."
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "node_modules/mime-types/**/*",
      "node_modules/mime-db/**/*"
    ],
    "extraResources": [
      {
        "from": "../Backend/dist/Thryve_back",
        "to": "backend",
        "filter": [
          "**/*",
          "!**/__pycache__/**",
          "!**/*.spec",
          "!**/*.pyc~",
          "!**/*.pyo~",
          "!**/*.git/**"
        ]
      }
    ],
    "extraMetadata": {
      "main": "electron/main.cjs"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        },
        {
          "target": "portable",
          "arch": ["x64"]
        }
      ],
      "icon": "../logo/icon.ico",
      "artifactName": "Thryve-${version}.${ext}"
    },
    "portable": {
      "artifactName": "Thryve-Portable-${version}.exe"
    },
    "mac": {
      "target": "dmg",
      "icon": "../logo/icon.ico",
      "artifactName": "Thryve-${version}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "icon": "../logo/icon.ico",
      "artifactName": "Thryve-${version}.${ext}"
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    },
    "afterPack": "electron/after-pack.js",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": false
    },
    "asar": true,
    "buildDependenciesFromSource": true,
    "removePackageScripts": true,
    "npmRebuild": true,
    "publish": null,
    "extends": null
  }
}
